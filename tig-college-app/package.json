{
  "name": "tig-college-app-monorepo",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r --filter ./apps/* run build",
    "dev:api": "pnpm -C apps/api dev",
    "dev:web": "pnpm -C apps/web dev",
    "typecheck": "tsc -p apps/api/tsconfig.json --noEmit && tsc -p apps/web/tsconfig.json --noEmit",
    "lint": "echo \"add your linter cmd here (eslint/biome)\" && exit 0"
  }
}
